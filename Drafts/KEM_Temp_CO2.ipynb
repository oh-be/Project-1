   "source": [
    "ice_co2_clean_df = ice_co2_df.iloc[:,[0,1]]\n",
    "ice_co2_clean_df = ice_co2_clean_df.rename(columns={\"ALL_50_full\":\"CO2(ppm)\"})\n",
    "# ice_co2_clean_df"
   ]
  },
