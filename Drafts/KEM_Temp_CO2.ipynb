    "# https://www.ncei.noaa.gov/access/paleo-search/?dataTypeId=7\n",
    "# CO2 concentration from ice core samples\n",
    "# CO2 data from 1000 to 2004\n",
    "\n",
    "file = \"smoothedco2.csv\"\n",
